//math object

console.log(Math);
console.log(Math.Pi);
console.log(Math.E);

const area= 7.7;

console.log(Math.round(area));
console.log(Math.floor(area));
console.log(Math.cell(area));
console.log(Math.trunc(area));


//random numbers

